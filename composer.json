{
    "name": "jaikumar0101/livewire-editor-input",
    "description": "Premium Laravel Livewire WYSIWYG Editor Package with Alpine.js - CKEditor 4, CKEditor 5, and TipTap support",
    "type": "library",
    "keywords": [
        "laravel",
        "livewire",
        "alpine",
        "alpinejs",
        "editor",
        "wysiwyg",
        "ckeditor",
        "ckeditor5",
        "tiptap",
        "rich-text-editor",
        "text-editor"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jaikumar",
            "email": "jaikumar43044@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Jaikumar0101\\LivewireEditor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jaikumar0101\\LivewireEditor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jaikumar0101\\LivewireEditor\\EditorServiceProvider"
            ],
            "aliases": {
                "LivewireEditor": "Jaikumar0101\\LivewireEditor\\Facades\\LivewireEditor"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
